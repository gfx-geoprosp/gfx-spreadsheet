{
  "name": "gfx-spreadsheet",
  "description": "React datagrid that is more focused on data",
  "version": "0.1.4-rc.1",
  "main": "core/gfxComponents.js",
  "module": "core/gfxComponents.esm.js",
  "types": "gfxComponents.d.ts",
  "author": "TNT Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gfx-geoprosp/gfx-spreadsheet.git"
